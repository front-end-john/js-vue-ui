{
  "name": "js-vue-ui",
  "version": "0.1.2",
  "author": "front-end-john",
  "main": "lib/js-vue-ui.umd.min.js",
  "description": "john 开发的第一个基于 vue-cli3 的 UI 组件库",
  "private": false,
  "keywords": [
    "js-vue-ui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:front-end-john/js-vue-ui.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name js-vue-ui --dest lib packages/index.js",
    "gulp-dev": "gulp --env dev"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "del": "^4.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "> 1%",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
